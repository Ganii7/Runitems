<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.16.0 -->
<interface>
  <!-- interface-name main.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="AdwWindow" id="main_window">
    <property name="default-height">188</property>
    <property name="default-width">188</property>
    <property name="height-request">200</property>
    <property name="resizable">False</property>
    <property name="width-request">360</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <property name="centering-policy">strict</property>
            <property name="title-widget">
              <object class="AdwViewSwitcherTitle" id="switcher_title">
                <property name="stack">stack</property>
                <property name="title">Runitems</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwViewStack" id="stack">
            <property name="vexpand">True</property>
            <child>
              <object class="AdwViewStackPage">
                <property name="child">
                  <object class="GtkBox">
                    <property name="can-focus">False</property>
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkButton" id="import_main_lane">
                        <property name="halign">baseline-center</property>
                        <property name="hexpand">True</property>
                        <property name="label">Import item build</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-end">48</property>
                        <property name="margin-start">48</property>
                        <property name="margin-top">48</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="label">This will import the most commonly used item build for each champion in the main lane.</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-end">48</property>
                        <property name="margin-start">48</property>
                        <property name="margin-top">24</property>
                        <property name="wrap">True</property>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="icon-name">user-home-symbolic</property>
                <property name="name">page1</property>
                <property name="title">Main lane</property>
              </object>
            </child>
            <child>
              <object class="AdwViewStackPage">
                <property name="child">
                  <object class="GtkBox">
                    <property name="can-focus">False</property>
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkButton" id="import_all_lane">
                        <property name="halign">baseline-center</property>
                        <property name="hexpand">True</property>
                        <property name="label">Import item build</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-end">48</property>
                        <property name="margin-start">48</property>
                        <property name="margin-top">48</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="label">This will import the most commonly used item build for each lane (top, jungle, mid, bottom, support) for every champion.</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-end">48</property>
                        <property name="margin-start">48</property>
                        <property name="margin-top">24</property>
                        <property name="wrap">True</property>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="icon-name">network-workgroup-symbolic</property>
                <property name="name">page2</property>
                <property name="title" translatable="yes">All lanes</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwViewSwitcherBar" id="switcher_bar">
            <property name="reveal" bind-source="switcher_title" bind-property="title-visible" bind-flags="sync-create"/>
            <property name="stack">stack</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
